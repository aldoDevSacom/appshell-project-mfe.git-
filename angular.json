{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "apps",
  "projects": {
    "app-shell": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/app-shell",
      "sourceRoot": "apps/app-shell/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-architects/native-federation:build",
          "options": {},
          "configurations": {
            "production": {
              "target": "app-shell:esbuild:production"
            },
            "development": {
              "target": "app-shell:esbuild:development",
              "dev": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-architects/native-federation:build",
          "options": {
            "target": "app-shell:serve-original:development",
            "rebuildDelay": 0,
            "dev": true,
            "port": 0
          }
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "apps/app-shell/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "apps/app-shell/public"
              }
            ],
            "styles": [
              "apps/app-shell/src/styles.css"
            ]
          }
        },
        "esbuild": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "apps/app-shell/src/main.ts",
            "polyfills": [
              "zone.js",
              "es-module-shims"
            ],
            "tsConfig": "apps/app-shell/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "apps/app-shell/public"
              }
            ],
            "styles": [
              "apps/app-shell/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all",
              "fileReplacements": []
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "apps/app-shell/src/environments/environment.ts",
                  "with": "apps/app-shell/src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-original": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "app-shell:esbuild:production"
            },
            "development": {
              "buildTarget": "app-shell:esbuild:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4200
          }
        }
      }
    },
